<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
		<add key="kafka" value="https://nuget.pkg.github.com/Fornari-Simone/index.json" protocolVersion="3" />
	</packageSources>
	<packageSourceMapping>
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
		<packageSource key="kafka">
			<package pattern="Utility.Kafka" />
			<package pattern="Character.ClientHTTP" />
			<package pattern="Resource.ClientHTTP" />
			<package pattern="Resource.Shared" />
			<package pattern="Character.Shared" />
		</packageSource>
	</packageSourceMapping>
	<packageSourceCredentials>
		<kafka>
			<add key="Username" value="Fornari-Simone" />
			<add key="ClearTextPassword" value="%TOKEN%" />
		</kafka>
	</packageSourceCredentials>
</configuration>
